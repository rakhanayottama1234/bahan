#!/bin/bash
clear
echo "pilih salah satu :"
echo '1. Move Data'
echo '2. Exit'
read -p "> " hehe1
if [ $hehe1 == 1 ]
then
clear
echo "-------------"
echo "Daftar File :"
ls
echo "-------------"
echo "# Mana yang ingin kamu pindahkan??"
read -p "> " hapus
mv $hapus /var/www/html
echo "Done... Terimakasih"
fi
if [ $hehe1 == 2 ]
then
exit
fi
